----------------------------------------------------ЗУ без владельца-----------------------------------------------------------------------------------------------
select count(*)
from registry.object_3767_ as land
left join 
	(
		select *
		from registry.object_4420_
		where is_deleted = 'f'
	) as rights 
	on land.attr_3774_ = rights.attr_4430_
where 
	land.is_deleted = 'f'    and 
	rights.object_id is null

 
 
 ------------------------------------------------ЗУ для дачного строительства без владельца-----------------------------------------------------------------------
select count(*) 
from registry.object_3767_ as land
left join 
	(
		select * 
		from registry.object_4420_
		where is_deleted = 'f'
	) rights 
	on land.attr_3774_ = rights.attr_4430_
where 
	land.is_deleted = 'f'    and 
	rights.object_id is null and
	(
		land.attr_3884_  SIMILAR to '%(д|Д)(а|А)(ч|Ч)(н|Н)%' or 
		land.attr_3883_  SIMILAR to '%(д|Д)(а|А)(ч|Ч)(н|Н)%'
	)


---------------------------------------------------------------ОКС без прав--------------------------------------------------------------------------------------
--------------------------------------------------------Площадные--------------------------------------------------------------------------------------------------
select count(*) as areal_without_owner
from registry.object_3407_ areal
left join 
	(
		select *
		from registry.object_4420_
		where is_deleted = 'f'
	) as rights_areal
	on areal.attr_3445_ = rights_areal.attr_4430_
where
		areal.is_deleted = 'f'    
	and
		rights_areal.object_id is null
